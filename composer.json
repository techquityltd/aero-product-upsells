{
    "name": "techquity/aero-cross-selling-module",
    "description": "A module to allow admin to add products to cross/up sell against other products",
    "authors": [
        {
            "name": "Jack Pickering",
            "email": "jack.pickering@techquity.co.uk"
        }
    ],
    "require": {
        "aerocommerce/core": "^0",
        "aerocommerce/admin": "^0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "AeroCrossSelling\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.aerocommerce.com"
        }
    ],
    "extra": {
        "laravel" : {
            "providers": [
                "AeroCrossSelling\\Providers\\ServiceProvider"
            ]
        }
    }
}
